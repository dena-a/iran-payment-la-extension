{
    "name": "dena-a/iran-payment-la-extension",
    "description": "IranPayment extension for laravel-admin",
    "type": "library",
    "homepage": "https://github.com/dena-a/iran-payment-la-extension",
    "license": "MIT",
    "keywords": ["laravel", "iran-payment", "laravel-admin", "extension"],
    "authors": [
        {
            "name": "Dena Ajdari",
            "homepage": "https://github.com/dena-a"
        }
    ],
    "require": {
        "php": ">=7.4",
        "dena-a/iran-payment": "~2.0",
        "encore/laravel-admin": "~1.6",
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Dena\\IranPayment\\LaravelAdmin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dena\\IranPayment\\LaravelAdmin\\LaravelAdminExtensionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
